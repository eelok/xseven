{
  "name": "xseven",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm run build:css && node app.js",
    "compile:sass": "node-sass sass/main.scss app/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' app/css/style.comp.css -o app/css/style.prefix.css",
    "compress:css": "node-sass app/css/style.prefix.css app/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2"
  }
}
